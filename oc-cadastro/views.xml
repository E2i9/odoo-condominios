<?xml version="1.0" encoding="utf-8"?>
<openerp>
  <data>
  	<!-- Criação do Menu Cadastro -->
  	<menuitem id="occ_Cadastro" name="Cadastro" />
  	
  	<!-- Action to open OCCadastro.Bloco -->
  	<act_window id="action_occ_bloco"
  		name="Registro de Blocos"
  		res_model="occ.bloco"
  		view_mode="tree,form"/>
  		
  	<!-- Menu item to open OCCadastro Bloco -->
  	<menuitem id="menu_occ_bloco"
  		name="Bloco"
  		parent="occ_Cadastro"
  		sequence="0"
  		action="action_occ_bloco" />
  </data>
  <record id="view_occ_bloco" model="ir.ui.view">
  	<field name="name">Bloco Form</field>
  	<field name="model">occ.bloco</field>
  	<field name="arch" type="xml">
  	
  		<form string="Nome do Bloco">
  			<field name="name"/>
  		</form>
  	</field>
  </record>
</openerp>