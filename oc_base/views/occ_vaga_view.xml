<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE openerp>
<openerp>
  <data>
  	<!-- Action to open OCCadastro.Vaga -->
  	<act_window id="action_occ_vaga" name="Registro de Vaga" res_model="occ.vaga" view_mode="tree,form"/>

  	<!-- Menu item to open OCCadastro Vaga -->
  	<menuitem id="menu_occ_vaga" name="Vaga" parent="oc_Register" sequence="4" action="action_occ_vaga"/>
  </data>
  <record id="view_tree_occ_vaga" model="ir.ui.view">
  	<field name="name">Vaga tree</field>
  	<field name="model">occ.vaga</field>
  	<field  name="arch" type="xml">
   		<tree>
   			<field name="name"/>
   			<field name="bloco_id"/>
   			<field name="tipo"/>	
   		</tree>
   	</field>
  </record>  <record id="view_form_occ_bloco" model="ir.ui.view">
  	<field name="name">Vaga Form</field>
  	<field name="model">occ.vaga</field>
  	<field  name="arch" type="xml">
   		<form string="Número da Vaga">
   			<header>
   				<!-- Aqui vão os botões -->
   			</header>
   			<sheet>
			  <group name="group_top">
      			<group name="group_left">
	  				<field name="bloco_id" widget="selection"/>
        			<field name="name"/>
	  				<field name="tipo" widget="radio"/>	
      			</group>
      			<group name="group_right">
					<!-- Aqui vai o conteúdo -->
      			</group>
    		  </group>
   			</sheet>
  		</form>
   	</field>
  </record>
</openerp>