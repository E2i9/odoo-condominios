<?xml version="1.0" encoding="utf-8"?>
<openerp>
  <data>
  	<!-- Action to open OCCadastro.Vaga -->
  	<act_window id="action_occ_vaga_carro" name="Registro de Vagas Carros" res_model="occ.vaga.carro" view_mode="tree,form"/>

  	<!-- Menu item to open OCCadastro Vaga -->
  	<menuitem id="menu_occ_vaga_carro" name="Vagas Carros" parent="oc_Register_Vagas" sequence="1" action="action_occ_vaga_carro"/>
  </data>
  <record id="view_tree_occ_vaga_carro" model="ir.ui.view">
  	<field name="name">Vaga Carro tree</field>
  	<field name="model">occ.vaga.carro</field>
  	<field  name="arch" type="xml">
   		<tree editable="True">
   			<field name="name"/>
        	<field name="bloco_id" widget="selection"/>
        	<field name="apto_id" widget="selection"/>
        	<field name="tag_ids"/>
   		</tree>
   	</field>
  </record>
  <record id="view_form_occ_vaga_carro" model="ir.ui.view">
  	<field name="name">Vaga Carro Form</field>
  	<field name="model">occ.vaga.carro</field>
  	<field  name="arch" type="xml">
   		<form string="Vaga Carro">
   			<header>
   				<!-- Aqui vão os botões -->
   			</header>
   			<sheet>
			  <group name="group_top">
      			<group name="group_left">
        			<field name="name"/>
        			<field name="tag_ids" widget="many2many_tags"/>
      			</group>
      			<group name="group_right">
        			<field name="bloco_id" widget="selection"/>
        			<field name="apto_id" widget="selection"/>
      			</group>
    		  </group>
   			</sheet>
  		</form>
   	</field>
  </record>
</openerp>